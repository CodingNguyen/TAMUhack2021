{
    "name": "MemeMachine",
    "description": "Meme extension to help you get through the day",
    "version": "1.0",
    "manifest_version": 2,
   
    "chrome_url_overrides": {
        "newtab": "Meme.html"
    },
    
    "permissions":["tabs", "activeTab"],
    
    "background": {
        "scripts": ["background.js"],
         "persistent": false
    }
}